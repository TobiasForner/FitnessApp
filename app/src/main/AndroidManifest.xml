<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fitnessapp3">

    <application
        android:name=".Startup"
        android:exported="false"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.DayNight">
        <activity android:name=".TimerActivity" />
        <activity android:name=".ManageExercisesActivity" />
        <activity android:name=".DurationExerciseActivity" />
        <activity android:name=".StartActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".WorkoutEditActivity" />
        <activity android:name=".ResumeWorkoutActivity" />
        <activity android:name=".AddWorkoutActivity" />
        <activity android:name=".AddExerciseActivity" />
        <activity android:name=".WorkoutActivity" />
        <activity android:name=".RestActivity" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>